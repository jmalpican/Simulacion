
<div class="modal fade" id="divModalCombo" tabindex="-1"
	role="dialog" aria-labelledby="h4TitleCombo">
	<div class="modal-dialog" role="document">
		<div class="modal-content" id="divModalContenidoCombo">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal"
					aria-label="Cerrar">
					<span aria-hidden="true">&times;</span>
				</button>
				<h4 class="modal-title" id="h4TitleCombo">
					<label id="lblTitleCombo"></label>
				</h4>
			</div>
			<div class="modal-body">
				<form class="form-horizontal">


					<div class="form-group">
						<input type="hidden" id="tabla" >
						<input type="hidden" id="txtCodigoCombo" >
						<label for="txtDescCombo" class="col-md-4 control-label">Descripcion:</label>
						<div class="col-md-8">
							<input type="text" class="form-control" id="txtDescCombo" placeholder="Descripcion">
						</div>
					</div>

				</form>



			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-primary" id="btnModalComboGuardar">Guardar</button>
				<button type="button" class="btn btn-default" id="btnModalComboCancel">Cancelar</button>
			</div>
		</div>
	</div>
</div>
