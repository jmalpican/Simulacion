
<div class="modal fade" id="divModalTabla" tabindex="-1"
	role="dialog" aria-labelledby="h4TitleTabla">
	<div class="modal-dialog" role="document">
		<div class="modal-content" id="divModalContenidoTabla">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal"
					aria-label="Cerrar">
					<span aria-hidden="true">&times;</span>
				</button>
				<h4 class="modal-title" id="h4TitleTabla">
					<label id="lblTitleTabla"></label>
				</h4>
			</div>
			<div class="modal-body">
				<form class="form-horizontal">


					<div class="form-group">
						<label for="txtDescTabla" class="col-md-4 control-label">Tabla:</label>
						<div class="col-md-8">
							<input type="text" class="form-control" id="txtDescTabla" placeholder="Nombre de la Tabla">
						</div>
					</div>

				</form>



			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-primary" id="btnModalTablaGuardar">Guardar</button>
				<button type="button" class="btn btn-default" id="btnModalTablaCancel">Cancelar</button>
			</div>
		</div>
	</div>
</div>
